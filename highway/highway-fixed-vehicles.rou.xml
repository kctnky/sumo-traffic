<?xml version="1.0" encoding="UTF-8"?>

<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    
    <!-- Define Vehicle Types -->
    <vType id="vNormal" vClass="passenger" color="yellow" length="5.0" minGap="2.5" accel="2.6" decel="4.5" maxSpeed="45" speedFactor="0.9" speedDev="0.2" sigma="0.5" />
  
    <!-- Define Routes -->
    <route id="rForward" edges="E0 E1 E2 E3 E4 E5 E6 E7 E8 E9 E10 E11 E12 E13" />
    <route id="rBackward" edges="-E13 -E12 -E11 -E10 -E9 -E8 -E7 -E6 -E5 -E4 -E3 -E2 -E1 -E0" />
	
	<vehicle id="0" type="vNormal" route="rForward" departEdge="0" depart="0" />
	<vehicle id="1" type="vNormal" route="rForward" departEdge="0" depart="5" />
	<vehicle id="2" type="vNormal" route="rForward" departEdge="0" depart="10" />	
    
</routes>
